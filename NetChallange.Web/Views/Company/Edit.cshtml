@model NetChallange.Web.Models.CompanyViewModel.CompanyCEViewModel
@{
    ViewBag.Title = "Şirket Güncelle";
}

@using (Html.BeginForm("EditConfirmed", "Company", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(p => p.Id)
        @await Html.PartialAsync("_CreateEdit")
    </div>
}

